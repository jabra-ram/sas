<div class="w-25 mx-auto p-4 mt-5 text-dark border border-dark">
  <h1 class="text-center"> Add Section</h1>
  <%= form_with model: @section do |f|%>
    <div class="mb-3">
      <%= f.label :section, class:"form-label" %><br>
      <%= f.text_field :section, class:"form-control" %>
    </div>
    <%= f.submit "Add", class:"btn btn-primary"%>
  <% end %>
</div>
